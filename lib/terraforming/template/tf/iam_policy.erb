<% iam_policies.each do |policy| -%>
  <%- version = iam_policy_version_of(policy) -%>
resource "aws_iam_policy" "<%= policy.policy_name %>" {
    name   = "<%= policy.policy_name %>"
    path   = "<%= policy.path %>"
    policy = <<POLICY
<%= prettify_policy(version.document, unescape: true) %>
POLICY
}

<% end -%>
